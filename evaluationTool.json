{
	"name": "call_outcomes",
	"description": "mark the following for the call",
	"parameters": {
		"type": "OBJECT",
		"properties": {
			"quality_score": {
				"type": "INTEGER",
				"description": "Call quality rating from 1-10",
				"minimum": 1,
				"maximum": 10
			},
			"issues": {
				"type": "ARRAY",
				"description": "Any issues encountered",
				"items": {
					"type": "STRING"
				}
			},
			"verified": {
				"type": "STRING",
				"enum": ["0", "1", "2", "3"],
				"description": "Verification outcome: 1=confirmed, 2=denied, 3=unexpected end, 0=unclear"
			}
		},
		"required": ["verified", "quality_score", "issues"]
	},
	"behavior": "BLOCKING"
}